{
	"name": "example",
	"version": "0.0.1",
	"description": "Example",
	"main": "./dist/main.bundle.js",
	"scripts": {
		"build": "webpack --mode=development",
		"build-production": "webpack --mode=production",
		"production": "npm run build-production && electron-builder build --win",
		"production:linux": "npm run build-production && electron-builder build --linux",
		"dev": "concurrently \"npm run react:dev\" \"npm run electron:dev\"",
		"react:dev": "webpack serve --mode=development --config ./webpack.react.js",
		"electron:dev": "nodemon --dev",
		"start": "npm run build && nodemon"
	},
	"build": {
		"appId": "be.example.app",
		"productName": "example",
		"copyright": "Copyright Â© 2022 Francesco Graulus",
		"directories": {
			"output": "installer"
		},
		"win": {
			"target": "nsis",
			"icon": "build/example.png"
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true
		},
		"linux": {
			"target": "deb",
			"maintainer": "Francesco Graulus",
			"icon": "build/example.png"
		},
		"deb": {
			"depends": [
				"gconf2",
				"gconf-service",
				"libnotify4",
				"libxtst6",
				"libnss3"
			]
		}
	},
	"keywords": [],
	"author": "Francesco Graulus",
	"license": "ISC",
	"devDependencies": {
		"@babel/preset-react": "^7.14.5",
		"@svgr/webpack": "^5.5.0",
		"@types/react": "^17.0.24",
		"@types/react-dom": "^17.0.9",
		"@types/styled-components": "^5.1.14",
		"@types/uuid": "^8.3.4",
		"concurrently": "^6.3.0",
		"electron": "^15.0.0",
		"electron-builder": "^22.11.7",
		"file-loader": "^6.2.0",
		"framer-motion": "^6.5.1",
		"html-webpack-plugin": "^5.3.2",
		"nodemon": "^2.0.13",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"styled-components": "^5.3.1",
		"ts-loader": "^9.2.6",
		"typescript": "^4.4.3",
		"url-loader": "^4.1.1",
		"webpack": "^5.56.0",
		"webpack-cli": "^4.8.0",
		"webpack-dev-server": "^4.3.0"
	},
	"dependencies": {
		"csvtojson": "^2.0.10",
		"electron-store": "^8.1.0",
		"react-chartjs-2": "^4.3.1",
		"react-router-dom": "^6.0.2",
		"uuid": "^8.3.2"
	}
}
